{
    "name": "letid/framework",
    "description": "Letid PHP Framework",
    "type": "framework",
    "license": "MIT",
    "homepage": "https://github.com/letid/framework",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/letid/framework.git"
        }
    ],
    "keywords": [
        "developer", "framework"
    ],
    "authors": [
        {
            "name": "Khen Solomon Lethil",
            "email": "khensolomon@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9"
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-0": {
        },
        "psr-4": {
            "Letid\\": ["Config/","Init/","Database/"]
        },
        "classmap": [],
        "files": []
    },
    "autoload-dev": {
        "classmap": []
    },
    "archive": {
        "exclude": [
            "/.git",
            "/composer.lock",
            "/composer.phar",
            "/*.gitattributes",
            "/*.gitignore",
            "/*.md"
        ]
    }
}
